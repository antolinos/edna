# Set up the environment
##export EDNA2HTMLDIR=/home/pjb/Diamond/EDNA2html
export EDNA2HTMLDIR=/home/pjb93/EDNA2html
# Run the EDNA2html.py script
python ${EDNA2HTMLDIR}/EDNA2html.py ${1+"$@"}
